{
    "private": true,
    "scripts": {
        "hot": "yarn clear:front && vite",
        "prod": "yarn clear:front && vite build",
        "hot-admin": "yarn clear:admin && vite --config vite.admin.config.ts",
        "prod-admin": "yarn clear:admin && vite build --config vite.admin.config.ts",
        "clear:admin": "rm -rf public/build/admin",
        "clear:front": "rm -rf public/build/front"
    },
    "devDependencies": {
        "laravel-vite-plugin": "^0.6.0",
        "sass": "^1.15.2",
        "vite": "^3.1.3"
    },
    "dependencies": {
        "gsap": "^3.3.4",
        "imask": "^7.6.1",
        "micromodal": "^0.4.10",
        "swiper": "^11.0.3"
    }
}
